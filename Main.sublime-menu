[
    {
        "caption": "Preferences",
        "mnemonic": "n",
        "id": "preferences",
        "children": [
            {
                "caption": "Package Settings",
                "mnemonic": "P",
                "id": "package-settings",
                "children": [
                    {
                        "caption": "SQL Prettify",
                        "children": [
                            {
                                "caption": "README",
                                "command": "open_file",
                                "args": {
                                    "file": "${packages}/SQL-Prettify/README.md"
                                }
                            },
                            {
                                "caption": "Settings",
                                "command": "edit_settings",
                                "args": {
                                    "base_file": "${packages}/SQL-Prettify/SQL-Prettify.sublime-settings",
                                    "default": "[\n\t$0\n]\n"
                                }
                            },
                            {
                                "caption": "Command Palette",
                                "command": "edit_settings",
                                "args":
                                {
                                    "base_file": "${packages}/SQL-Prettify/Default.sublime-commands",
                                    "default": "[\n\t$0\n]\n"
                                }
                            },
                            {
                                "caption": "Key Bindings",
                                "command": "edit_settings",
                                "args": {
                                    "base_file": "${packages}/SQL-Prettify/Default ($platform).sublime-keymap",
                                    "user_file": "${packages}/User/Default ($platform).sublime-keymap",
                                    "default": "[\n\t$0\n]\n"
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
]
